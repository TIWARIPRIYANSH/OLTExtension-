{
  "manifest_version": 3,
  "name": "AI Helper",
  "version": "1.2",
  "description": "Extracts questions from a page and gets answers.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "*://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}